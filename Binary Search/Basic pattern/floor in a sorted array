class Solution {
    public int findFloor(int[] arr, int x) {
        int l=0, r=arr.length-1;
        int ans = -1;
        while(l<=r){
            int mid = (l+r)/2;
            if(arr[mid] <= x){
            ans = mid; 
            l = mid+1;
            }
            else
             r = mid-1;
        }
        return ans;
    }
}
