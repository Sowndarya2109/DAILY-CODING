class Solution {
    static ArrayList<Integer> leaders(int arr[]) {
        ArrayList<Integer> arr1 = new ArrayList<>();
        int n = arr.length;
        int max = arr[n-1];
        arr1.add(max);
        for(int i=n-2; i>=0; i--){
            if(arr[i] >= max){
                max = arr[i];
                arr1.add(max);
            }
        }
         Collections.reverse(arr1);     
        return arr1;
    }
}
