class Solution {
    public int minimumPrefixLength(int[] nums) {
        int n = nums.length;
        int count = n-1;
        for(int i=n-1; i>=1; i--){
            if(nums[i] - nums[i-1] == 1){
                 count--;
            }
            else if(nums[i] > nums[i-1]){
                count--;   
            }
            else{
                break;
            }                                       
        }
        return count;
    }
}
