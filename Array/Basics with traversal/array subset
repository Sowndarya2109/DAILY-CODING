
class Solution {
    public boolean isSubset(int a[], int b[]) {
        HashMap<Integer, Integer> arr = new HashMap<>();
        for(int x:a){
            arr.put(x, arr.getOrDefault(x,0)+1);
        }
        for(int x:b){
            if(!arr.containsKey(x) || arr.get(x) == 0)
            return false;
            arr.put(x, arr.get(x)-1);
        }
        return true;
    }
}
