class Solution {
    public boolean isPalindrome(int n) {
        String num = Integer.toString(n);
        if(num.length() == 1)
        return true;
        int len = num.length();
        for(int i=0; i<len/2; i++){
                if(num.charAt(i) != num.charAt(len-1-i))
                return false;
            }
        return true;
    }
}
