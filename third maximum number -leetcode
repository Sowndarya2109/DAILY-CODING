class Solution {
    public int thirdMax(int[] nums) {
        Set<Integer> set = new HashSet<>();
        for(int x : nums){
            set.add(x);
        }
        Integer arr[] = set.toArray(new Integer[0]);
        Arrays.sort(arr);
        if(arr.length < 3)
        return arr[arr.length-1];
        else
        return arr[arr.length-3];
    }
}
